{
  "name": "vanilla-drag-movable-demo",
  "version": "1.0.0",
  "description": "An example program for vanilla-drag-movable",
  "main": "index.ts",
  "repository": "https://github.com/tai2/vanilla-drag-movable",
  "scripts": {
    "preinstall": "cd .. && yarn install",
    "build": "run-s build:lib build:demo",
    "build:lib": "cd .. && yarn build",
    "build:demo": "parcel build index.html",
    "watch": "run-p watch:lib watch:demo",
    "watch:lib": "cd .. && yarn watch",
    "watch:demo": "parcel index.html"
  },
  "author": "Taiju Muto <hello@tai2.net>",
  "license": "MIT",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4"
  }
}
